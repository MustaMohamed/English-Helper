<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.musta.englishhelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:icon="@drawable/ic_app"
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddNewListActivity"
            android:label="@string/title_activity_add_new_list"
            android:parentActivityName=".AddNewListActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ShowWordDetails"
            android:parentActivityName=".ShowWordDetails" />
        <activity
            android:name=".ShowMyList"
            android:label="@string/title_activity_main2"
            android:parentActivityName=".ShowMyList"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".QuickTestActivity"
            android:parentActivityName=".QuickTestActivity"></activity>
    </application>

</manifest>